@model WorktimeRegister.Models.UserProfile

<tr>
    <td class="lalign">
        @Html.DisplayFor(modelItem => Model.UserName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => Model.Email)
    </td>
    <td>
        @Html.DisplayFor(modelItem => Model.FirstName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => Model.LastName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => Model.PhoneNumber)
    </td>
    <td>
        @Html.ActionLink("Edit", "EditUserInfo", new { id = Model.UserId }) |
        @Html.ActionLink("Delete", "Delete", new { id = Model.UserId }) |
        @Html.ActionLink("Worktimes", "Worktimes", new { id = Model.UserId })
    </td>
</tr>